<h1>Editing cms_page</h1>
<p><%= link_to 'Show', admin_cms_page_path(@cms_page) %> | <%= link_to 'Back', admin_cms_pages_path %></p>

<% form_for(:cms_page, @cms_page, :url => { :action => :update }, :html => {:method => :put}) do |f| %>

<%= render :partial => "cms_page_fields", :locals => { :f => f } %>

<%= f.submit 'Update' %>

<% end %>

